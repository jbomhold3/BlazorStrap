

<DynamicElement TagName="a" class="@classname">
    @ChildContent
</DynamicElement>

@code {
    private string classname => 
        new CssBuilder("navbar-brand")
            .AddClass(Class)
        .Build();
    
    [Parameter] private string Class { get; set; }
    [Parameter] private RenderFragment ChildContent { get; set; }
}