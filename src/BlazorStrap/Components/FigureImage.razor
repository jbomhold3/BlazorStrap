@inherits BootstrapComponentBase

<DynamicElement TagName="img" class="@classname" style="@Style" @attributes="UnknownParameters"/>

@code {
    string classname =>
        new CssBuilder("figure-img img-fluid rounded")
            .AddClass(Class)
        .Build();

    [Parameter] private string Class { get; set; }
    [Parameter] private string Style { get; set; }
}